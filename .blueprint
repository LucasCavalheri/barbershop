created:
    - database/factories/UserFactory.php
    - database/factories/BarbershopFactory.php
    - database/factories/ServiceFactory.php
    - database/factories/AppointmentFactory.php
    - database/migrations/2025_04_28_123824_create_users_table.php
    - database/migrations/2025_04_28_123825_create_barbershops_table.php
    - database/migrations/2025_04_28_123826_create_services_table.php
    - database/migrations/2025_04_28_123827_create_appointments_table.php
    - database/migrations/2025_04_28_123828_create_barbershop_user_table.php
    - app/Models/User.php
    - app/Models/Barbershop.php
    - app/Models/Service.php
    - app/Models/Appointment.php
models:
    User: { name: string, email: 'string:unique', password: string, role: 'enum:client,barber', phone: 'string:nullable', timestamps: timestamps, relationships: { belongsToMany: Barbershop } }
    Barbershop: { name: string, address: string, phone: string, timestamps: timestamps, relationships: { belongsToMany: User, hasMany: 'Service Appointment' } }
    Service: { barbershop_id: 'foreign:barbershops', name: string, duration: integer, price: decimal, timestamps: timestamps, relationships: { belongsTo: Barbershop, hasMany: Appointment } }
    Appointment: { client_id: 'foreign:users', barber_id: 'foreign:users', barbershop_id: 'foreign:barbershops', service_id: 'foreign:services', start_time: datetime, end_time: datetime, status: 'enum:pending,confirmed,canceled', timestamps: timestamps, relationships: { belongsTo: 'User as client User as barber Barbershop Service' } }
